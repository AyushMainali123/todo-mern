  });\n\n      resetForm();\n      setSubmitting(false);\n    })\n    .catch((error) => {\n      console.log(error);\n      setSubmitting(false);\n    });\n};\n\nexport const deleteUser = (id) => (dispatch) => {\n  axios.delete(`user/delete/${id}`).then((response) => {\n    console.log(response);\n    const { _id: id, name } = response.data;\n    dispatch({\n      type: DELETE_USER,\n      payload: {\n        id,\n      },\n    });\n\n    axios.delete(`todo/deleteByUserName/${name}`)\n      .then(response => {\n      console.log(response)\n      dispatch({\n        type: DELETE_TODO_BY_USERNAME,\n        payload: {\n          user: name,\n        },\n      });\n    })\n\n  });\n};\n","/home/n1/Documents/todo-mern/todo-frontend/src/actions/todoActions.js",[],"/home/n1/Documents/todo-mern/todo-frontend/src/reducers/todoReducer.js",[],"/home/n1/Documents/todo-mern/todo-frontend/src/reducers/userReducer.js",[],"/home/n1/Documents/todo-mern/todo-frontend/src/components/TodoCard.js",["88"],"import React from \"react\";\nimport { Card, CardActions, CardContent, Button } from \"@material-ui/core\";\nimport \"./TodoCard.css\";\nimport { useDispatch } from \"react-redux\";\nimport { removeTodo } from \"../actions/todoActions\";\nconst TodoCard = ({ id, user, item, description, startDate }) => {\n  const dispatch = useDispatch();\n  const deleteTodo = () => {\n    dispatch(removeTodo(id));\n  };\n  const resDate = new Date(startDate).toDateString()\n  return (\n    <div className=\"todoCard\">\n      <Card>\n        <CardContent>\n          <div className=\"todoCard__createdBy\">\n            <div>\n              Todo Created By {user} ({resDate})\n            </div>\n          </div>\n          <div className=\"todoCard__todo\">{item}</div>\n          <div className=\"todoCard__description\">{description}</div>\n          <div className=\"todoCard__actionButtons\">\n            <Button\n              variant=\"contained\"\n              color=\"primary\"\n              style=